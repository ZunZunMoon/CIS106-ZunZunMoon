    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="carcompanyormake" type="String" array="False" size=""/>
            <declare name="model" type="String" array="False" size=""/>
            <declare name="msrpamount" type="Real" array="False" size=""/>
            <declare name="discountpercent" type="Real" array="False" size=""/>
            <declare name="amountoffmsrp" type="Real" array="False" size=""/>
            <declare name="discountedprice" type="Real" array="False" size=""/>
