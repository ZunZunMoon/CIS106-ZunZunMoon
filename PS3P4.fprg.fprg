            <output expression="&quot; enter decimal number for dicount percent &quot; &amp;discountpercent" newline="True"/>
            <output expression="&quot; The amount off msrp &quot; &amp;amountoffmsrp" newline="True"/>
            <output expression="&quot; The discounted price &quot; &amp;discountedprice" newline="True"/>
